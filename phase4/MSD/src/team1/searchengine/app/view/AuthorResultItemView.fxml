<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team1.searchengine.app.controller.AuthorResultItemController">
   <children>
      <VBox>
         <children>
            <HBox alignment="BOTTOM_LEFT" fillHeight="false" spacing="10.0">
               <children>
                  <Hyperlink fx:id="hlnkAuthorName" onAction="#onLinkClicked" text="[AuthorName]" wrapText="true">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Hyperlink>
                  <Label fx:id="lblAlias" layoutY="20.0" text="[alias]">
                     <HBox.margin>
                        <Insets bottom="3.0" />
                     </HBox.margin></Label>
               </children>
            </HBox>
            <VBox minHeight="0.0" minWidth="0.0">
               <children>
                  <FlowPane fx:id="fpneAuthorDetails" minHeight="0.0">
                     <children>
                        <Pane fx:id="pneIsShortlisted" minHeight="0.0" prefHeight="20.0" prefWidth="19.0">
                           <children>
                              <ImageView fitHeight="17.0" fitWidth="17.0" layoutX="1.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@shortlist_enabled.png" />
                                 </image>
                              </ImageView>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsEditor" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="3.0" style="-fx-background-color: #4ba8ff;" styleClass="rounded" text="# Editor" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsGeneralChair" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="3.0" style="-fx-background-color: #9a84e0;" styleClass="rounded" text="# General Chair" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsProgramChair" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="2.0" style="-fx-background-color: #be63b3;" styleClass="rounded" text="# Program Chair" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsConferenceChair" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="3.0" style="-fx-background-color: #dd6d6d;" styleClass="rounded" text="# Conference Chair" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsExternalReviewCommittee" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="2.0" style="-fx-background-color: #679a56;" styleClass="rounded" text="# External Review" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                        <Pane fx:id="pneIsCommitteeMember" minHeight="0.0">
                           <children>
                              <Label layoutX="5.0" layoutY="2.0" style="-fx-background-color: #d29f1e;" styleClass="rounded" text="# Committe Member" textFill="WHITE">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </Label>
                           </children>
                           <padding>
                              <Insets left="5.0" right="5.0" />
                           </padding>
                        </Pane>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </FlowPane>
               </children>
               <padding>
                  <Insets bottom="5.0" />
               </padding>
            </VBox>
            <Separator prefWidth="200.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets left="15.0" />
   </padding>
</VBox>
